<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<data>
		
		<record id="product_customer_ref_form" model="ir.ui.view">
			<field name="name">product.customer.ref.form</field>
			<field name="model">product.customer.ref</field>
			<field name="arch" type="xml">
				<form string="Customer reference">
					<sheet>
						<group>
							<field name="partner_id"/>
							<field name="name"/>
						</group>
					</sheet>
				
				</form>
			</field>
		</record>
		
		<record id="product_customer_ref_tree" model="ir.ui.view">
			<field name="name">product.customer.ref.tree</field>
			<field name="model">product.customer.ref</field>
			<field name="arch" type="xml">
				<tree string="Custormer reference">
					<field name="partner_id"/>
					<field name="name"/>
				</tree>
			</field>
		</record>
	
	
	</data>
</odoo>